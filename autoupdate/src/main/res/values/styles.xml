<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="primaryButton" parent="Widget.AppCompat.Button">
        <item name="android:layout_height">32dp</item>
        <item name="android:textColor">@color/icons</item>
        <item name="android:textSize">@dimen/button</item>
        <item name="android:background">@drawable/dialog_btn_ok_bg</item>
        <item name="background">@drawable/dialog_btn_ok_bg</item>
    </style>

    <style name="autoUpdateDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:minWidth">240dp</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
</resources>